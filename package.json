{
  "name": "@claasahl/spotware-protobuf",
  "version": "0.2.0",
  "description": "Protocol Buffers and Data Models for Spotware Open API 2.0",
  "main": "./build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "prepare": "npm run build",
    "build": "npm run import && npm run gen:json && tsc",
    "preimport": "shx rm -rf ./assets/*.proto && shx mkdir ./spotware-messages",
    "import": "cd ./spotware-messages && isogit clone --url=https://github.com/spotware/Open-API-2.0-protobuf-messages --singleBranch",
    "postimport": "shx cp ./spotware-messages/*.proto ./assets && shx rm -rf ./spotware-messages",
    "gen:json": "ts-node src/hello-world.ts -i ./assets -o ./src",
    "postgen:json": "prettier --write ./src/*.json"
  },
  "keywords": [
    "spotware",
    "ctrader",
    "typescript",
    "protobuf"
  ],
  "author": {
    "name": "Claas Ahlrichs",
    "email": "claasahl@gmail.com",
    "url": "https://github.com/claasahl"
  },
  "repository": "github:claasahl/spotware-protobuf",
  "bugs": {
    "url": "https://github.com/claasahl/spotware-protobuf/issues"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/globby": "^9.1.0",
    "@types/node": "^11.13.6",
    "command-line-args": "^5.1.1",
    "globby": "^9.2.0",
    "husky": "^1.3.1",
    "isomorphic-git": "^0.54.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "protobufjs": "^6.8.8",
    "shx": "^0.3.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  },
  "engines": {
    "node": ">= 10.5.0",
    "npm": ">= 6.4.1"
  }
}
